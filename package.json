    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "formidable": "^3.5.2",
    "jsonwebtoken": "^9.0.2",
    "mitata": "^1.0.34",
    "mysql2": "^3.11.5",
